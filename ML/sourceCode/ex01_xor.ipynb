   "source": [
    "from sklearn import svm\n",
    "\n",
    "clf = svm.SVC(gamma=\"scale\")\n",
    "clf.fit([\n",
    "    [0,0],  # 숫자이거나, 숫자로 형변환 가능해야 함.\n",
    "    [0,1],\n",
    "    [1,0],\n",
    "    [1,1]\n",
    "], [0,1,1,0])\n",
    "results = clf.predict([ \n",
    "        [0,0],\n",
    "        [1,0]\n",
    "])\n",
    "print(results)"
   ]
